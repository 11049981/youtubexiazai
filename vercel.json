{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/downloads/(.*)",
            "dest": "/downloads/$1"
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
} 